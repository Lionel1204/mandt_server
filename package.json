{
  "name": "mandt-server",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "start-local": "NODE_ENV=local node ./bin/www",
    "start-dev": "NODE_ENV=development node ./bin/www",
    "start-staging": "NODE_ENV=staging node ./bin/www",
    "start": "NODE_ENV=production node ./bin/www",
    "db:create": "npx sequelize db:create --env local",
    "db:migrate": "npx sequelize db:migrate --env local",
    "db:migrate:undo": "npx sequelize db:migrate:undo --env local",
    "db:drop": "npx sequelize db:drop --env local",
    "db:seeds": "npx sequelize db:seed:all --env local",
    "db:seed": "npx sequelize db:seed --seed=20220703060234-login_records.js --env local"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "ajv-keywords": "^5.1.0",
    "body-parser": "^1.20.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cos-nodejs-sdk-v5": "^2.11.12",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-session": "^1.17.3",
    "http-errors": "~1.6.3",
    "lodash": "^4.17.21",
    "log4js": "^6.5.2",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "nconf": "^0.12.0",
    "qrcode": "^1.5.0",
    "redis": "^4.2.0",
    "sequelize": "^6.12.0-alpha.1",
    "sequelize-cli": "^6.3.0",
    "shortid": "^2.2.16",
    "svg-captcha": "^1.4.0",
    "svg64": "^1.1.0",
    "url-parse": "^1.5.10",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "crypto": "^1.0.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "minimist": "^1.2.6",
    "prettier": "^2.6.2"
  }
}
